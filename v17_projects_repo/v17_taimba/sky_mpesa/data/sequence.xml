<odoo>
    <data noupdate="0">
    <!--    SEQUENCE FOR MPESA-->
        <record id="loading_sequences" model="ir.sequence">
            <field name="name">Mpesa Sequence</field>
            <field name="code">mpesa.sequences</field>
            <field name="prefix">MPS/%(year)s/</field>
            <field name="suffix"></field>
            <field name="padding">5</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
        </record>
    </data>
</odoo>